# Database - For Vite frontend, must prefix with VITE_
VITE_DATABASE_URL="your-neon-database-url"

# Admin API Key - For Vite frontend, must prefix with VITE_
VITE_ADMIN_API_KEY="your-secret-admin-key"

# For serverless functions (non-prefixed)
DATABASE_URL="your-neon-database-url"
ADMIN_API_KEY="your-secret-admin-key"

# WebSocket server URL for multiplayer functionality
# For development, use ws://localhost:8080
# For production, use your Railway deployment URL (wss://your-app.railway.app)
VITE_WEBSOCKET_URL="ws://localhost:8080"